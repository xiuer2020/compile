.type{
  display: flex;
  align-items: center;
  box-sizing: border-box;
  border-radius: inherit;
  height: 160rpx;
  padding: 32rpx;
}
.left{
  border-bottom: 1rpx solid #dadada;
  border-right: 1rpx solid #dadada;
}
.right{
  border-bottom: 1rpx solid #dadada;
}

.icon{
  margin-right: 16rpx;
}

.title{
  color: #272727;
}

.description{
  margin-top: 8rpx;
  font-size: 28rpx;
  color: #9c9898;
}
